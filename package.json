{
    "name": "OneDegree",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "start": "react-native start",
        "test": "jest",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "build-and": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ../",
        "build-ios": "react-native run-ios --configuration Release",
        "bundle-and": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ../",
        "prettier-fix": "prettier --write */.js",
        "prettier-check": "prettier --list-different */.js",
        "lint-fix": "eslint --fix .",
        "lint-check": "eslint .",
        "rename": "react-native-rename",
        "postinstall": "npx jetify",
        "clean": "cd android && ./gradlew clean && ./gradlew cleanBuildcache && cd .. && cd ios && rm -rf build/ && xcodebuild clean && watchman watch-del-all && cd ..",
        "pod": "cd ios && pod install && cd ..",
        "pod-m1": "cd ios && sudo arch -x86_64 gem install ffi && arch -x86_64 pod install && cd ..",
        "pod-update-m1": "cd ios && sudo arch -x86_64 gem install ffi && arch -x86_64 pod install && arch -x86_64 pod update",
        "pod_update_m1": "cd ios && arch -x86_64 sudo gem install cocoapods && arch -x86_64 pod repo update && arch -x86_64 pod install",
        "pod-update": "pod --repo-update",
        "clean:ios": "cd ios && xcodebuild clean && cd ../",
        "clean:android": "cd android && ./gradlew clean && cd ../",
        "generate:ts": "graphql-codegen --config codegen.yml"
    },
    "dependencies": {
        "@invertase/react-native-apple-authentication": "^2.1.5",
        "@ptomasroos/react-native-multi-slider": "^2.2.2",
        "@react-native-async-storage/async-storage": "^1.17.3",
        "@react-native-community/checkbox": "^0.5.8",
        "@react-native-community/datetimepicker": "^3.5.2",
        "@react-native-community/masked-view": "0.1.10",
        "@react-native-community/push-notification-ios": "^1.10.1",
        "@react-native-firebase/app": "^14.9.0",
        "@react-native-firebase/auth": "^14.9.0",
        "@react-native-firebase/crashlytics": "^14.9.0",
        "@react-native-firebase/messaging": "^14.9.0",
        "@react-native-google-signin/google-signin": "^7.2.2",
        "@react-native-picker/picker": "^1.16.5",
        "@react-navigation/bottom-tabs": "^6.3.1",
        "@react-navigation/drawer": "^6.1.3",
        "@react-navigation/native": "^6.0.2",
        "@react-navigation/stack": "5.14.3",
        "appcenter": "4.4.3",
        "appcenter-analytics": "4.4.3",
        "appcenter-crashes": "4.4.3",
        "axios": "^0.27.2",
        "color": "^3.1.3",
        "formik": "^2.2.9",
        "graphql": "^16.4.0",
        "graphql-request": "^4.2.0",
        "react": "16.13.1",
        "react-devtools": "^4.13.5",
        "react-native": "0.63.4",
        "react-native-actions-sheet": "^0.6.1",
        "react-native-animatable": "^1.3.3",
        "react-native-chart-kit": "^6.11.0",
        "react-native-config": "^1.4.5",
        "react-native-confirmation-code-field": "^7.3.0",
        "react-native-drawer": "^2.5.1",
        "react-native-elements": "^3.4.2",
        "react-native-fbsdk-next": "^8.0.2",
        "react-native-flash-message": "^0.2.0",
        "react-native-gesture-handler": "1.10.3",
        "react-native-image-picker": "^4.0.6",
        "react-native-keyboard-aware-scroll-view": "^0.9.5",
        "react-native-linear-gradient": "^2.5.6",
        "react-native-media-controls": "^2.3.0",
        "react-native-modal-datetime-picker": "^10.2.0",
        "react-native-pager-view": "^5.4.15",
        "react-native-picker-select": "^8.0.4",
        "react-native-push-notification": "^8.1.1",
        "react-native-reanimated": "1.13.2",
        "react-native-responsive-fontsize": "^0.5.0",
        "react-native-safe-area-context": "3.1.9",
        "react-native-screens": "^2.18.1",
        "react-native-skeleton-placeholder": "^3.0.4",
        "react-native-slider": "^0.11.0",
        "react-native-snap-carousel": "^3.9.1",
        "react-native-step-indicator": "^1.0.3",
        "react-native-svg": "^12.1.1",
        "react-native-swiper": "^1.6.0",
        "react-native-uuid": "^2.0.1",
        "react-native-vector-icons": "^8.1.0",
        "react-native-video": "^5.2.0",
        "react-query": "^3.34.19",
        "rn-fetch-blob": "^0.12.0",
        "validate.js": "^0.13.1",
        "yup": "^0.32.11",
        "zustand": "^3.7.2"
    },
    "devDependencies": {
        "@babel/core": "7.13.8",
        "@babel/plugin-proposal-decorators": "7.13.5",
        "@babel/runtime": "7.13.8",
        "@react-native-community/eslint-config": "2.0.0",
        "@types/color": "^3.0.1",
        "@types/react": "^17.0.3",
        "@types/react-native": "^0.63.51",
        "babel-jest": "26.6.3",
        "babel-plugin-module-resolver": "^4.1.0",
        "eslint": "7.21.0",
        "jest": "26.6.3",
        "metro-react-native-babel-preset": "0.65.1",
        "react-test-renderer": "16.13.1",
        "typescript": "^4.2.3"
    },
    "jest": {
        "preset": "react-native"
    }
}
